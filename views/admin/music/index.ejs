<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <link rel="stylesheet" href="/essentials/admin/css.css"/>
        <link rel="stylesheet" href="/essentials/admin/input.css"/>
        <script src="/essentials/admin/nav.js"></script>
        <script src="https://unpkg.com/@popperjs/core@2"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        <!-- UIkit CSS -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.9.4/dist/css/uikit.min.css" />

        <!-- UIkit JS -->
        <script src="https://cdn.jsdelivr.net/npm/uikit@3.9.4/dist/js/uikit.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/uikit@3.9.4/dist/js/uikit-icons.min.js"></script>

        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">



        <title>Music Records</title>
    </head>
    <body class="bodyflex">



        <div class="NavigatorHam">
            <div style="height:4px; width: 100%;"></div>



            <%- Naver %> 



        </div>

        <div class="bodie">

            <div class="marg-10">
                
                <div class="Buttoneste">
                    <a class="uk-button uk-button-primary" href="/admin/music/upload">Upload</a>
                </div>

                <table class="uk-table uk-table-divider">
                    <thead>
                        <tr>
                            <th>Audio Name</th>
                            <th>Game Name</th>
                            <th>Views</th>
                            <th>Downloads</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>

                        <% for( let x = 0; x < audioData.length; x++ ) { %>

                            <tr>
                                <td><%= audioData[x]["audioName"] %></td>
                                <td><%= audioData[x]["gameName"] %></td>
                                <td><%= audioData[x]["viewCount"] %></td>
                                <td><%= audioData[x]["downloadCount"] %></td>
                                <td>
                                    <a class="uk-button uk-button-primary" href="/admin/music/edit/<%= audioData[x]["id"] %>"><i class="bi bi-pencil-square"></i></a>
                                    <a class="uk-button uk-button-danger" href="/admin/music/del/<%= audioData[x]["id"] %>"><i class="bi bi-trash-fill"></i></a>
                                </td>
                            </tr>

                        <% } %>

                    </tbody>
                </table>



            </div>

        </div>
    </body>
</html>